    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "vscode:package": "vsce package",
    "vscode:ls": "vsce ls"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "~18.19.31",
    "@types/vscode": "^1.87.2",
