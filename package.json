    "@typescript-eslint/parser": "^7.8.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^2.26.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "vscode": "^1.87.2"
